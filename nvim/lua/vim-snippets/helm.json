  "PersistentVolumeClaim_spec": {
    "prefix": "PersistentVolumeClaim_spec",
    "body": [
      "kind: PersistentVolumeClaim",
      "apiVersion: v1",
      "metadata:",
      "  name: {{ template \"$2.$1.fullname\" . \\}\\}",
      "  labels:",
      "\tapp: {{ template \"${2:chartName}.name\" . \\}\\}",
      "\tchart: \"{{ .Chart.Name \\}\\}-{{ .Chart.Version \\}\\}\"",
      "\trelease: \"{{ .Release.Name \\}\\}\"",
