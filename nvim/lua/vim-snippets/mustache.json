{
  "if": {
    "prefix": "if",
    "description": "# {{#value}} ... {{/value}}",
    "body": [
      "{{#${1:value}\\}\\}",
      "${0:$TM_SELECTED_TEXT}",
      "{{/$1\\}\\}"
    ]
  },
  "ifn": {
    "prefix": "ifn",
    "description": "# {{^value}} ... {{/value}}",
    "body": [
      "{{^${1:value}\\}\\}",
      "${0:$TM_SELECTED_TEXT}",
      "{{/$1\\}\\}"
    ]
  },
  "ife": {
    "prefix": "ife",
    "description": "# {{#value}} ... {{/value}} {{^value}} ... {{/value}}",
    "body": [
      "{{#${1:value}\\}\\}",
      "${2:$TM_SELECTED_TEXT}",
      "{{/$1\\}\\}",
      "{{^$1\\}\\}",
      "$3",
      "{{/$1\\}\\}"
    ]
  }
}
