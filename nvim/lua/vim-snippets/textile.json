{
  "header": {
    "prefix": "header",
    "body": [
      "---",
      "title: ${1:title}",
      "layout: post",
      "date: ${2:date} ${0:hour:minute:second} -05:00",
      "---",
      ""
    ]
  },
  "img": {
    "prefix": "img",
    "body": [
      "!${1:url}(${2:title}):${0:link}!",
      ""
    ]
  },
  "|": {
    "prefix": "|",
    "body": [
      "|$1|",
      ""
    ]
  },
  "link": {
    "prefix": "link",
    "body": [
      "\"${1:link text}\":${0:url}",
      ""
    ]
  },
  "(": {
    "prefix": "(",
    "body": [
      "(${1:Expand acronym})",
      ""
    ]
  },
  "fn": {
    "prefix": "fn",
    "body": [
      "[${1:ref number}] $0",
      "",
      "fn$1. ${2:footnote}",
      ""
    ]
  }
}
